{
  "name": "issueflow",
  "version": "1.0.0",
  "description": "Find unassigned open GitHub issues without pull requests - your gateway to open-source contribution",
  "type": "module",
  "author": "ascender1729 <https://github.com/ascender1729>",
  "license": "MIT",
  "homepage": "https://issueflow.pages.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/VibeTensor/IssueFlow.git"
  },
  "bugs": {
    "url": "https://github.com/VibeTensor/IssueFlow/issues"
  },
  "keywords": [
    "github",
    "issues",
    "open-source",
    "contribution",
    "pull-requests",
    "hacktoberfest",
    "astro",
    "svelte",
    "github-api",
    "graphql"
  ],
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@astrojs/svelte": "^7.2.0",
    "@unocss/astro": "^66.5.3",
    "astro": "^5.14.3",
    "graphql-request": "^7.2.0",
    "rough-notation": "^0.5.1",
    "svelte": "^5.39.11",
    "unocss": "^66.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@vitest/ui": "^4.0.9",
    "happy-dom": "^20.0.10",
    "playwright": "^1.56.1",
    "vitest": "^4.0.9"
  }
}
